{
    "name": "vscode-okpy",
    "displayName": "VSCode okpy",
    "description": "Provides support for the ok Python autograder",
    "version": "1.2.0",
    "publisher": "aopell",
    "icon": "img/icon.png",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:okpy.question",
        "onCommand:okpy.questionUnlock",
        "onCommand:okpy.allUnlock",
        "onCommand:okpy.all",
        "onCommand:okpy.submit"
    ],
    "contributes": {
        "commands": [
            {
                "command": "okpy.question",
                "title": "Grade Question",
                "category": "OK"
            },
            {
                "command": "okpy.questionUnlock",
                "title": "Unlock Question",
                "category": "OK"
            },
            {
                "command": "okpy.allUnlock",
                "title": "Unlock All",
                "category": "OK"
            },
            {
                "command": "okpy.all",
                "title": "Grade All",
                "category": "OK"
            },
            {
                "command": "okpy.submit",
                "title": "Submit",
                "category": "OK"
            }
        ]
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/aopell/vscode-okpy.git"
    }
}
